{
  "name": "quickstart-webpack",
  "version": "1.0.0",
  "description": "A scaffold project for a Webpack based webapplication, without the overhead of specific libraries.",
  "main": "index.js",
  "scripts": {
    "dist": "webpack --config webpack.prod.js",
    "start": "webpack-dev-server --config webpack.dev.js",
    "clean": "del-cli dist/bundle.js dist/bundle.js.map *.tgz",
    "lint": "eslint ./src --ext .js,.jsx",
    "prepack": "npm run clean && npm run dist"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@babel/preset-env": "^7.8.0",
    "babel-loader": "^8.0.0",
    "css-loader": "^3.4.0",
    "del-cli": "^3.0.0",
    "eslint": "^6.8.0",
    "file-loader": "^5.1.0",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "style-loader": "^1.1.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^3.0.0",
    "webpack": "^4.42.0",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.10.0",
    "webpack-merge": "^4.2.0"
  }
}
